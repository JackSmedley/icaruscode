  #
  { Name: "icaruspmtwetop01"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwetop02"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwetop03"  TriggerDelay: "43 ns" },
  #
  # bottom
  #
  { Name: "icaruspmtwebot01"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwebot02"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwebot03"  TriggerDelay: "43 ns" },
