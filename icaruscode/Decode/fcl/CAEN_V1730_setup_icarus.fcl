  # top
  #
  { Name: "icaruspmtwwtop01"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwwtop02"  TriggerDelay: "43 ns" },
  { Name: "icaruspmtwwtop03"  TriggerDelay: "43 ns" },
  #
  # bottom
